@import '../libs/nib'

header
  background-color #111
  width 100%
  height 43px
  color #fff

.content-wrapper
  padding 20px

.primer
  background-color #ccc
  width 200px
  height 200px

.parent
  position relative

.animation-primer
  width 100px
  height 100px
  background-color #336E7B
  position absolute
  top 0
  left 0
  backface-visibility: hidden;
  animation firstAnimation 2s ease-in-out infinite alternate

@keyframes firstAnimation
  0%
    top 0
    left 0
    transform rotate(0deg)
  25%
    transform rotate(10deg)
  50%
    transform rotate(0deg) translate
  75%
    transform rotate(-10deg)
  100%
    top 0
    left 300px
    transform rotate(0deg)